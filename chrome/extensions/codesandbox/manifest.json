{
    "manifest_version": 2,
    "name": "codesandbox",
    "version": "1.0.0",
    "description": "codesandbox",
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_title": "codesandbox"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "options_page": "html/options.html",
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "permissions": [
        "activeTab",
        "declarativeContent", 
        "storage"
    ]
}